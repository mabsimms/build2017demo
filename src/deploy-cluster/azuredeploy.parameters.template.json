{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "dnsNamePrefix": {
      "value": "##DNS_PREFIX##"
    },
    "agentCount": {
      "value":##AGENT_COUNT##
    },
    "agentVMSize": {
      "value": "Standard_D3_v2"
    },
    "linuxAdminUsername": {
      "value": "##USERNAME##"
    },
    "orchestratorType": {
      "value": "Swarm"
    },
    "masterCount": {
      "value": 3
    },
    "sshRSAPublicKey": {
      "value": "##SSH_PUBLIC_KEY##"
    }
  }
}
