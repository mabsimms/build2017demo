Version": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "SwarmMode"
    },
    "masterProfile": {
      "count": 3,
      "dnsPrefix": "##DNS_PREFIX##",
      "vmSize": "Standard_D2_v2"
    },
    "agentPoolProfiles": [
      {
        "name": "agentpublic",
        "count": ##AGENT_COUNT##,
        "vmSize": "Standard_D3_v2",
        "dnsPrefix": "",
        "ports": [
          80,
          443,
          8080
        ]
      }
    ],
    "linuxProfile": {
      "adminUsername": "##USERNAME##",
      "ssh": {
        "publicKeys": [
          {
            "keyData": "##SSH_PUBLIC_KEY##"
          }
        ]
      }
    }
  }
}
